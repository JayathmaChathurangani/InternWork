<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://wso2.org/bpmn/repositoryCreationProcess">
  <process id="repositoryCreationProcess" name="Repository Creation Process" isExecutable="true">
    <startEvent id="repositoryCreationStart" name="Start Repository Creation"></startEvent>
    <serviceTask id="gitHubRepoRequestAddedIntoDbRestTask" name="GitHub Repository Request Added into Database" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/databaseService/insertData</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"tableName":"${tableName}","columns":"${columns}","data":${data}}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputMappings">
          <activiti:string>outputType:type,outputMessage:message</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <userTask id="approveGitHubREpositoryUserTask" name="Waiting for approval" activiti:assignee="admin"></userTask>
    <serviceTask id="createGitHubRepositoryRestTask" name="Create GitHub Repository" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/gitHubService/getSample</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"columns":"${mailColumns}","data":"${data}"}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputMappings">
          <activiti:string>outputType:type</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow1" sourceRef="repositoryCreationStart" targetRef="gitHubRepoRequestAddedIntoDbRestTask"></sequenceFlow>
    <serviceTask id="sendRequestForApprovalRestTask" name="Send Request for Approval via E-mail" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/mailService/sendMail</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"columns":${mailColumns},"data":${data}}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputMappings">
          <activiti:string>outputType:type,outputMessage:message</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow5" sourceRef="gitHubRepoRequestAddedIntoDbRestTask" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow6" name="Database insersion completed" sourceRef="exclusivegateway1" targetRef="sendRequestForApprovalRestTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow7" name="Database insersion failed" sourceRef="exclusivegateway1" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Error"}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="deleteInsertedDataFromDbRestTask" name="Roll Back Data" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/databaseService/deleteData</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"tableName":"${tableName}","condition":"WHERE ${columns[0]}= ${data[0]} "}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputVariable">
          <activiti:string>output</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow8" sourceRef="sendRequestForApprovalRestTask" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow9" name="E-mail sending failed" sourceRef="exclusivegateway2" targetRef="deleteInsertedDataFromDbRestTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Error"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" name="E-mail sending completed" sourceRef="exclusivegateway2" targetRef="approveGitHubREpositoryUserTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="deleteInsertedDataFromDbRestTask" targetRef="endevent1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow12" sourceRef="approveGitHubREpositoryUserTask" targetRef="exclusivegateway3"></sequenceFlow>
    <sequenceFlow id="flow13" name="Approved" sourceRef="exclusivegateway3" targetRef="createGitHubRepositoryRestTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask1" name="Send acknowledgement" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/mailService/sendAckMail</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"to":${data[10]},"messageData":"Your repository request rejected"}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputVariable">
          <activiti:string>ack</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow14" name="Rejected" sourceRef="exclusivegateway3" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Reject"}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask2" name="Send acknowledgement" activiti:async="true" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/mailService/sendAckMail</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"to":${data[10]},"messageData":"Your repository request succesfully completed."}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputVariable">
          <activiti:string>ack</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow16" sourceRef="servicetask2" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow17" sourceRef="servicetask1" targetRef="endevent1"></sequenceFlow>
    <serviceTask id="servicetask3" name="Send acknowledgement" activiti:async="true" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/mailService/sendAckMail</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"to":${data[10]},"messageData":"Your repository request accepted. But repository creation fails"}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputVariable">
          <activiti:string>ack</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow21" sourceRef="servicetask3" targetRef="endevent1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway4" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow22" sourceRef="createGitHubRepositoryRestTask" targetRef="exclusivegateway4"></sequenceFlow>
    <sequenceFlow id="flow23" name="Repository creation completed" sourceRef="exclusivegateway4" targetRef="servicetask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow24" name="Repository creation failed" sourceRef="exclusivegateway4" targetRef="servicetask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Error"}]]></conditionExpression>
    </sequenceFlow>
  </process>
