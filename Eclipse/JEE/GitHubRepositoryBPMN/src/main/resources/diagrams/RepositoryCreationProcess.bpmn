      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="mailtask1" name="Send Mail for approval" isForCompensation="true" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${sendToList}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:expression><![CDATA[${data[11]}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Repository Request]]></activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression><![CDATA[<p>
<b>${mailColumns[0]}</b>:${mailData[0]}<br />
<b>${mailColumns[1]}</b>:${mailData[1]}<br />
<b>${mailColumns[2]}</b>:${mailData[2]}<br />
<b>${mailColumns[3]}</b>:${mailData[3]}<br />
<b>${mailColumns[4]}</b>:${mailData[4]}<br />
<b>${mailColumns[5]}</b>:${mailData[5]}<br />
