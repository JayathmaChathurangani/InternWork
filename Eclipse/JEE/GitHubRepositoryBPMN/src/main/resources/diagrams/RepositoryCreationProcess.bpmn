<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://wso2.org/bpmn/repositoryCreationProcess">
  <process id="repositoryCreationProcess" name="Repository Creation Process" isExecutable="true">
    <startEvent id="repositoryCreationStart" name="Start Repository Creation"></startEvent>
    <serviceTask id="gitHubRepoRequestAddedIntoDbRestTask" name="GitHub Repository Request Added into Database" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/databaseService/repository/insertData</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"data":["${data[0]}","${data[1]}","${data[2]}","${data[3]}","${data[4]}","${data[5]}","${data[6]}","${data[7]}","${data[8]}","${data[9]}","${data[10]}","${data[11]}"]}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputMappings">
          <activiti:string>outputType:responseType,outputMessage:responseMessage,repositoryId:repositoryId</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <userTask id="approveGitHubREpositoryUserTask" name="Waiting for approval" activiti:assignee="admin"></userTask>
    <serviceTask id="createGitHubRepositoryRestTask" name="Create Repositories" isForCompensation="true" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/createRepositories</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"repositoryId":${repositoryId}}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputMappings">
          <activiti:string>outputTypeGitHub:responseType,repoUpdatedDetails:repoUpdatedDetails,outputTypeJenkins:responseDefault,outputTypeNexus:responseDefault</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow1" sourceRef="repositoryCreationStart" targetRef="gitHubRepoRequestAddedIntoDbRestTask"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow5" sourceRef="gitHubRepoRequestAddedIntoDbRestTask" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow7" name="Database insersion failed" sourceRef="exclusivegateway1" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Error"}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow12" sourceRef="approveGitHubREpositoryUserTask" targetRef="exclusivegateway3"></sequenceFlow>
    <sequenceFlow id="flow13" name="Approved" sourceRef="exclusivegateway3" targetRef="createGitHubRepositoryRestTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="mailtask1" name="Send Mail for approval" isForCompensation="true" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${sendToList}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:expression><![CDATA[${data[11]}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Repository Request]]></activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression><![CDATA[<p>
Hi all!<br /><br />

There is a repository request with following details. <br /><br />

<li><b>${mailColumns[0]}</b>:&nbsp;${mailData[0]}</li>
<li><b>${mailColumns[1]}</b>:&nbsp;${mailData[1]}</li>
<li><b>${mailColumns[2]}</b>:&nbsp;${mailData[2]}</li>
<li><b>${mailColumns[3]}</b>:&nbsp;${mailData[3]}</li>
<li><b>${mailColumns[4]}</b>:&nbsp;${mailData[4]}</li>
<li><b>${mailColumns[6]}</b>:&nbsp;${mailData[6]}</li>
<li><b>${mailColumns[7]}</b>:&nbsp;${mailData[7]}</li>
<li><b>${mailColumns[8]}</b>:&nbsp;${mailData[8]}</li>
<li><b>${mailColumns[9]}</b>:&nbsp;${mailData[9]}</li>
<li><b>${mailColumns[10]}</b>:&nbsp;${mailData[10]}</li>
<li><b>${mailColumns[11]}</b>:&nbsp;${mailData[11]}</li>
<li><b>${mailColumns[5]}</b>:&nbsp;<br/><br/>&emsp;${mailData[5].replaceAll("\\\\\\\\n","<br/>&emsp;")}</li><br/>

<strong>In order to accept or reject the request please click the following button.</strong>
<br /><br/>
<li><b>Link:&nbsp;<b /><a href=http://localhost:3000/root/acceptRepository?repositoryId=${repositoryId}><button>Click Me</button></a>
</p>]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow25" sourceRef="exclusivegateway1" targetRef="mailtask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow26" sourceRef="mailtask1" targetRef="approveGitHubREpositoryUserTask"></sequenceFlow>
    <serviceTask id="mailtask2" name="Send Acknowledement" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${sendToList} ${data[11]}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[project@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Repository Rejected]]></activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression><![CDATA[<p>
Hi all!<br /><br />

There is a repository request with following details. <br /><br />

<li><b>${mailColumns[0]}</b>:&nbsp;${mailData[0]}</li>
<li><b>${mailColumns[1]}</b>:&nbsp;${mailData[1]}</li>
<li><b>${mailColumns[2]}</b>:&nbsp;${mailData[2]}</li>
<li><b>${mailColumns[3]}</b>:&nbsp;${mailData[3]}</li>
<li><b>${mailColumns[4]}</b>:&nbsp;${mailData[4]}</li>
<li><b>${mailColumns[6]}</b>:&nbsp;${mailData[6]}</li>
<li><b>${mailColumns[7]}</b>:&nbsp;${mailData[7]}</li>
<li><b>${mailColumns[8]}</b>:&nbsp;${mailData[8]}</li>
<li><b>${mailColumns[9]}</b>:&nbsp;${mailData[9]}</li>
<li><b>${mailColumns[10]}</b>:&nbsp;${mailData[10]}</li>
<li><b>${mailColumns[11]}</b>:&nbsp;${mailData[11]}</li>
<li><b>${mailColumns[5]}</b>:&nbsp;<br/><br/>&emsp;${mailData[5].replaceAll("\\\\\\\\n","<br/>&emsp;")}</li><br/>

</p>]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow27" name="Reject" sourceRef="exclusivegateway3" targetRef="mailtask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Reject"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow28" sourceRef="mailtask2" targetRef="endevent1"></sequenceFlow>
    <serviceTask id="mailtask4" name="Send Acknowledgement" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${sendToList} ${data[11]}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression><![CDATA[<p>
<b>${mailColumns[0]}</b>:${repoUpdatedDetails.REPOSITORY_NAME}<br />
<b>${mailColumns[1]}</b>:${repoUpdatedDetails.REPOSITORY_LANGUAGE}<br />
<b>${mailColumns[2]}</b>:${repoUpdatedDetails.REPOSITORY_BUILDABLE}<br />
<b>${mailColumns[3]}</b>:${repoUpdatedDetails.REPOSITORY_NEXUS}<br />
<b>${mailColumns[4]}</b>:${repoUpdatedDetails.REPOSITORY_PRIVATE}<br />
<b>${mailColumns[5]}</b>:${repoUpdatedDetails.REPOSITORY_DESCRIPTION}<br />
<b>${mailColumns[6]}</b>:${repoUpdatedDetails.REPOSITORY_GROUPID}<br />
<b>${mailColumns[7]}</b>:${repoUpdatedDetails.LICENSE_NAME}<br />
<b>${mailColumns[8]}</b>:${repoUpdatedDetails.TEAM_NAME}<br />
<b>${mailColumns[9]}</b>:${repoUpdatedDetails.ORGANIZATION_NAME}<br />
<b>${mailColumns[10]}</b>:${repoUpdatedDetails.REPOSITORYTYPE_NAME}<br />
<b>${mailColumns[11]}</b>:${repoUpdatedDetails.REPOSITORY_REQUEST_BY}<br />


</p>]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[project@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Repository Creation sucessfull]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow32" sourceRef="mailtask4" targetRef="endevent1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Set Issue Template" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/gitHub/setIssueTemplate</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"organization":"${repoUpdatedDetails.ORGANIZATION_NAME}","repositoryName":"${repoUpdatedDetails.REPOSITORY_NAME}"}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputMappings">
          <activiti:string>outputTypeGitHubIssue:responseType</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow33" sourceRef="createGitHubRepositoryRestTask" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputTypeGitHub == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask2" name="Create Nexus Repository Target" activiti:async="true" isForCompensation="true" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/createNexusRepositoryTarget</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"groupId":"${repoUpdatedDetails.REPOSITORY_GROUPID}","contentClass":"maven2"}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputMappings">
          <activiti:string>outputTypeNexus:responseType</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask3" name="Create Jenkins" activiti:async="true" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/createJenkins</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"name":"${repoUpdatedDetails.REPOSITORY_NAME}","id":"${repoUpdatedDetails.REPOSITORY_ID}"}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputMappings">
          <activiti:string>outputTypeJenkins:responseType</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <inclusiveGateway id="inclusivegateway2" name="Inclusive Gateway"></inclusiveGateway>
    <sequenceFlow id="flow46" sourceRef="inclusivegateway2" targetRef="servicetask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${repoUpdatedDetails.REPOSITORY_BUILDABLE == true && outputTypeGitHub == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow47" sourceRef="inclusivegateway2" targetRef="servicetask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${repoUpdatedDetails.REPOSITORY_NEXUS == true && outputTypeGitHub == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask4" name="Set Pull Request Template" isForCompensation="true" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/gitHub/setPullRequestTemplate</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"organization":"${repoUpdatedDetails.ORGANIZATION_NAME}","repositoryName":"${repoUpdatedDetails.REPOSITORY_NAME}"}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputMappings">
          <activiti:string>outputTypeGitHubPullRequest:responseType</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow48" sourceRef="servicetask1" targetRef="servicetask4"></sequenceFlow>
    <serviceTask id="servicetask5" name="Set ReadMe file" activiti:class="org.wso2.carbon.bpmn.extensions.rest.RESTTask" activiti:extensionId="org.wso2.developerstudio.bpmn.extensions.restTask.RESTTask">
      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://localhost:9090/gitHub/setReadMe</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>POST</activiti:string>
        </activiti:field>
        <activiti:field name="input">
          <activiti:expression>{"organization":"${repoUpdatedDetails.ORGANIZATION_NAME}","repositoryName":"${repoUpdatedDetails.REPOSITORY_NAME}","repositoryDescription":"${repoUpdatedDetails.REPOSITORY_DESCRIPTION}"}</activiti:expression>
        </activiti:field>
        <activiti:field name="outputMappings">
          <activiti:string>outputTypeGitHubReadMe:responseType</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow49" sourceRef="servicetask4" targetRef="servicetask5"></sequenceFlow>
    <sequenceFlow id="flow50" sourceRef="servicetask5" targetRef="inclusivegateway2"></sequenceFlow>
    <inclusiveGateway id="inclusivegateway3" name="Inclusive Gateway"></inclusiveGateway>
    <sequenceFlow id="flow51" sourceRef="servicetask2" targetRef="inclusivegateway3"></sequenceFlow>
    <sequenceFlow id="flow53" sourceRef="inclusivegateway3" targetRef="mailtask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputTypeGitHub == "Done" && outputTypeJenkins == "Done" &&  outputTypeNexus == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow59" sourceRef="servicetask3" targetRef="inclusivegateway3"></sequenceFlow>
  </process>
