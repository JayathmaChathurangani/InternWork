<b>${mailColumns[10]}</b>:${mailData[10]}<br />
<b>${mailColumns[11]}</b>:${mailData[11]}<br />

<br />
<b>Link : <b />http://localhost:3000/root/acceptRepository?repositoryId=${repositoryId}
</p>]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow25" sourceRef="exclusivegateway1" targetRef="mailtask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType == "Done"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow26" sourceRef="mailtask1" targetRef="approveGitHubREpositoryUserTask"></sequenceFlow>
    <serviceTask id="mailtask2" name="Send Acknowledement" activiti:type="mail">
